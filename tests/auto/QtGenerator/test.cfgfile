			{tagScalar
				{valueType QString}
				{name string_field}
				{oneOfConstraint "\"StringStringStringStringStringStringStringStringStringStringStringStringStringStringStringStringStringStringStringString\""}
				{required}
			}
		}

		{class tags_t
